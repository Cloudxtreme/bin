#!/bin/bash

######### START CONFIGURATION #########
. moo_config

######### START FUNCTIONS #########
. moo_functions

arches="32 64"
PWD=$(pwd)
for mm in $arches
do
	cd ${MOO_HOME}abs/mooOS-pkgs-$mm/
	mv $(ls | grep "efl-git-1" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "efl-git-debug-1" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "elementary-git-1" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "elementary-git-debug-1" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "elementary_test-git-1" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "elementary_test-git-debug-1" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "enlightenment17-git-0" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "enlightenment17-git-debug" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "evas_generic_loaders-git-1" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "evas_generic_loaders-git-debug-1" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "pulseaudio-mixer-cli-git" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "pyradio-git" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "sqlmap-git" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "terminology-git-0" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "terminology-git-debug-0" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "urxvt-font-size-git" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
	mv $(ls | grep "youtube-viewer-git" | sort -r | head -2) ${MOO_HOME}abs/staging_$mm/
done
cd "$PWD"

exit 0

pkgver=$(LC_ALL=C date -u +%Y%m%d)
cd /tmp
rm blacklist.txt
wget https://projects.parabolagnulinux.org/blacklist.git/plain/blacklist.txt -O blacklist.txt

cut -d: -f1,2 blacklist.txt | \
      sed "s/:$//" | \
      grep -v ":" | \
      #grep -E -o '.{0,99}:' | \
      #sed "s/://g" | \
      grep -v '^#' | \

      ## example cheat line
      sed "s/steam/zteam/g" | \
      sed "s/nvidia/zvidia/g" | \
      ## end

      #sed "s/:/ /g" | \
      sort -u > list_a

less list_a

exit 0