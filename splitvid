#!/bin/bash

VID_FILENAME="$1"

ffmpeg -i "$VID_FILENAME" -ss 00:00:00 -t 00:06:00 -c copy 1-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 00:06:00 -t 00:06:00 -c copy 2-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 00:12:00 -t 00:06:00 -c copy 3-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 00:18:00 -t 00:06:00 -c copy 4-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 00:24:00 -t 00:06:00 -c copy 5-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 00:30:00 -t 00:06:00 -c copy 6-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 00:36:00 -t 00:06:00 -c copy 7-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 00:42:00 -t 00:06:00 -c copy 8-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 00:48:00 -t 00:06:00 -c copy 9-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 00:54:00 -t 00:06:00 -c copy 10-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 01:00:00 -t 00:06:00 -c copy 11-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 01:06:00 -t 00:06:00 -c copy 12-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 01:12:00 -t 00:06:00 -c copy 13-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 01:18:00 -t 00:06:00 -c copy 14-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 01:24:00 -t 00:06:00 -c copy 15-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 01:30:00 -t 00:06:00 -c copy 16-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 01:36:00 -t 00:06:00 -c copy 17-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 01:42:00 -t 00:06:00 -c copy 18-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 01:48:00 -t 00:06:00 -c copy 19-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 01:54:00 -t 00:06:00 -c copy 20-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 02:00:00 -t 00:06:00 -c copy 21-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 02:06:00 -t 00:06:00 -c copy 22-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 02:12:00 -t 00:06:00 -c copy 23-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 02:18:00 -t 00:06:00 -c copy 24-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 02:24:00 -t 00:06:00 -c copy 25-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 02:30:00 -t 00:06:00 -c copy 26-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 02:36:00 -t 00:06:00 -c copy 27-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 02:42:00 -t 00:06:00 -c copy 28-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 02:48:00 -t 00:06:00 -c copy 29-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 02:54:00 -t 00:06:00 -c copy 30-"$VID_FILENAME"
ffmpeg -i "$VID_FILENAME" -ss 03:00:00 -t 00:08:40 -c copy 31-"$VID_FILENAME"

exit 0
